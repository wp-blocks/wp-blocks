{
	"name": "wp-blocks",
	"version": "0.1.0",
	"private": true,
	"description": "A monorepo of tools and blocks for the WordPress Gutenberg editor.",
	"license": "GPL-2.0-or-later",
	"contributors": [
		"John Hooks <bitmachina@outlook.com> (https://johnhooks.io/)"
	],
	"homepage": "https://github.com/wp-blocks/wp-blocks#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/wp-blocks/wp-blocks.git"
	},
	"bugs": "https://github.com/wp-blocks/wp-blocks/issues",
	"type": "module",
	"sideEffects": false,
	"workspaces": [
		"./packages/*"
	],
	"scripts": {
		"packages-update": "wp-scripts packages-update",
		"build": "turbo build --filter=./packages/*",
		"build:types": "turbo build:types --filter=./packages/*",
		"build:withTypes": "npm run build && npm run build:types",
		"check": "tsc -b",
		"format": "turbo format --filter=./packages/*",
		"lint:css": "turbo lint:css --filter=./packages/*",
		"lint:js": "turbo lint:js ",
		"wp-env:start": "wp-env start",
		"wp-env:stop": "wp-env stop",
		"wp-env:destroy": "wp-env destroy",
		"test": "jest --silent=false --coverage",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@babel/core": "^7.21.0",
		"@babel/preset-typescript": "^7.21.0",
		"@jest/globals": "^29.4.3",
		"@types/babel__core": "^7.20.0",
		"@types/expect-puppeteer": "^5.0.3",
		"@types/jest": "^29.4.0",
		"@types/jest-environment-puppeteer": "^5.0.3",
		"@types/node": "^18.14.1",
		"@types/puppeteer": "^7.0.4",
		"@typescript-eslint/eslint-plugin": "^5.53.0",
		"@typescript-eslint/parser": "^5.53.0",
		"@wordpress/e2e-test-utils": "^9.4.0",
		"@wordpress/e2e-tests": "^6.4.0",
		"@wordpress/env": "^5.12.0",
		"@wordpress/jest-preset-default": "^10.8.0",
		"@wordpress/prettier-config": "^2.10.0",
		"@wordpress/scripts": "^25.4.0",
		"@wordpress/stylelint-config": "^21.10.0",
		"@wp-blocks/tsconfig": "workspace:^0.1.0",
		"babel-jest": "^29.4.3",
		"eslint-import-resolver-typescript": "^3.5.3",
		"eslint-plugin-import": "^2.27.5",
		"husky": "^8.0.3",
		"jest-puppeteer": "^6.2.0",
		"lint-staged": "^13.1.2",
		"turbo": "^1.8.1",
		"typescript": "^4.9.5"
	}
}
