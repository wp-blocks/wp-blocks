{
	"name": "@wp-blocks/create-block",
	"version": "0.1.0",
	"description": "Generates PHP, TS and CSS code for registering a block for a WordPress plugin.",
	"contributors": [
		"John Hooks <bitmachina@outlook.com> (https://johnhooks.io/)"
	],
	"license": "GPL-2.0-or-later",
	"keywords": [
		"wordpress",
		"gutenberg",
		"block",
		"scaffold"
	],
	"homepage": "https://github.com/wp-blocks/wp-blocks#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/wp-blocks/wp-blocks.git",
		"directory": "packages/create-block"
	},
	"bugs": {
		"url": "https://github.com/wp-blocks/wp-blocks/issues"
	},
	"engines": {
		"node": ">=14",
		"npm": ">=6.14.4"
	},
	"scripts": {
		"build:pkg": "../../scripts/build-pkg.js",
		"build:esm": "../../scripts/build-esm.js",
		"build:compile": "../../scripts/build-compile.js",
		"build:types": "../../scripts/build-types.js",
		"check": "tsc --build ./src",
		"clean": "rimraf ./{dist,temp}",
		"clean:all": "yarn clean && yarn clean:tsc",
		"clean:tsc": "rimraf ./build"
	},
	"files": [
		"build"
	],
	"type": "module",
	"source": "src/index.ts",
	"module": "./build/index.js",
	"exports": "./build/index.js",
	"types": "./build/index.d.ts",
	"bin": {
		"wp-create-block": "./build/index.js"
	},
	"dependencies": {
		"@wordpress/lazy-import": "^1.14.0",
		"@wp-blocks/utils": "workspace:^0.1.0",
		"chalk": "^4.0.0",
		"change-case": "^4.1.2",
		"check-node-version": "^4.1.0",
		"commander": "^9.2.0",
		"execa": "^4.0.2",
		"fast-glob": "^3.2.7",
		"inquirer": "^7.1.0",
		"make-dir": "^3.0.0",
		"mustache": "^4.0.0",
		"npm-package-arg": "^8.1.5",
		"prompts": "^2.4.2",
		"rimraf": "^3.0.2",
		"write-pkg": "^4.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@types/npm-package-arg": "^6.1.1"
	}
}
