{
	"name": "@wp-blocks/wp-update",
	"version": "0.2.0",
	"description": "Utility to update dependencies on Gutenberg to a specific release.",
	"contributors": [
		"John Hooks <bitmachina@outlook.com> (https://johnhooks.io/)"
	],
	"license": "GPL-2.0-or-later",
	"keywords": [
		"wordpress",
		"gutenberg"
	],
	"homepage": "https://github.com/wp-blocks/wp-blocks/tree/main/packages/wp-update#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/wp-blocks/wp-blocks.git",
		"directory": "packages/wp-update"
	},
	"bugs": {
		"url": "https://github.com/wp-blocks/wp-blocks/issues"
	},
	"engines": {
		"node": ">=14.16",
		"pnpm": ">=7.14.1"
	},
	"scripts": {
		"build:pkg": "../../scripts/build-pkg.js",
		"build:esm": "../../scripts/build-esm.js",
		"build:compile": "../../scripts/build-compile.js",
		"build:types": "../../scripts/build-types.js",
		"check": "tsc --build ./src",
		"clean": "rimraf ./{build,temp}",
		"clean:all": "pnpm clean && pnpm clean:tsc",
		"clean:tsc": "rimraf ./lib"
	},
	"files": [
		"bin.js",
		"build"
	],
	"type": "module",
	"source": "./src/index.ts",
	"module": "./build/index.js",
	"exports": "./build/index.js",
	"types": "./build/index.d.ts",
	"bin": "./bin.js",
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"node-fetch": "^3.3.1",
		"picocolors": "^1.0.0",
		"prettier": "^2.8.4",
		"yargs": "^17.7.1"
	},
	"devDependencies": {
		"@types/yargs": "^17.0.22"
	}
}
