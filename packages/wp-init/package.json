{
	"name": "wp-init",
	"version": "0.1.2",
	"description": "Utility to scaffold parts of WordPress block editor projects.",
	"contributors": [
		"John Hooks <bitmachina@outlook.com> (https://johnhooks.io/)"
	],
	"license": "GPL-2.0-or-later",
	"keywords": [
		"wordpress",
		"gutenberg",
		"block",
		"initializer",
		"scaffold"
	],
	"homepage": "https://github.com/wp-blocks/wp-blocks/tree/main/packages/wp-init#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/wp-blocks/wp-blocks.git",
		"directory": "packages/wp-init"
	},
	"bugs": {
		"url": "https://github.com/wp-blocks/wp-blocks/issues"
	},
	"engines": {
		"node": ">=14.16",
		"pnpm": ">=7.14.1"
	},
	"scripts": {
		"build:pkg": "wp-blocks build pkg",
		"build:esm": "wp-blocks build esm",
		"build:compile": "wp-blocks build compile --local",
		"build:types": "wp-blocks build types --local",
		"check": "tsc --build ./src",
		"clean": "rimraf ./{build,temp}",
		"clean:all": "pnpm clean && pnpm clean:tsc",
		"clean:tsc": "rimraf ./lib"
	},
	"files": [
		"bin.js",
		"build"
	],
	"type": "module",
	"source": "./src/index.ts",
	"module": "./build/index.js",
	"exports": "./build/index.js",
	"types": "./build/index.d.ts",
	"bin": "./bin.js",
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"@clack/prompts": "^0.6.3",
		"@wp-blocks/utils": "workspace:*",
		"picocolors": "^1.0.0",
		"yargs": "^17.7.1"
	},
	"devDependencies": {
		"@types/yargs": "^17.0.22",
		"@wp-blocks/scripts": "workspace:*"
	}
}
